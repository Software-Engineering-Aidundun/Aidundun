@import "main.wxss";
@import "icon.wxss";

.news-list{
  min-height: 400rpx;
  padding: 10rpx;
}
.news-item{
  display: flex;
  flex-direction: row;
  margin: 9rpx;
  background-color:white;
  border-radius: 12rpx;
  border: 1rpx solid rgb(170, 173, 175);
}
.news-item image{
  height: 120rpx;
  width: 150rpx;
  margin: 10rpx;
}
.news-item text{
  width: 100%;
  line-height: 50rpx;
  font-size: 30rpx;
}
